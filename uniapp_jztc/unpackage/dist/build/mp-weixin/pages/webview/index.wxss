.webview-container{position:relative;width:100%;height:100vh}.loading{position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);display:flex;flex-direction:column;align-items:center;z-index:50}.loading-spinner{width:60rpx;height:60rpx;border:4rpx solid #f0f0f0;border-radius:50%;border-top-color:#fc3e2b;animation:spin 1s linear infinite;margin-bottom:20rpx}.loading-text{font-size:28rpx;color:#666}@keyframes spin{0%{transform:rotate(0)}to{transform:rotate(360deg)}}
